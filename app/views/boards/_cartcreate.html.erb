<div class="modal fade" id="createCart" tabindex="-1" role="dialog" aria-labelledby="myModalLabel" aria-hidden="true">
    <div class="modal-dialog" role="document">
      <div class="modal-content col mt-5 p-5 text-info">
        <h2 class="mb-4">新增Card</h2>
        <%= notice %>
        <%= form_with model: Card.new do |card| %>
          <div class="form-group">
            <%= card.label :title, "標題" %>
            <%= card.text_field :title , class: 'form-control' %>
          </div>
          <div class="form-group d-flex justify-content-end">
            <%= card.submit %>
          </div>
        <% end %>
      </div>
    </div>
  </div>
  